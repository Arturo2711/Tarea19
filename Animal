// Clase abstracta Animal
abstract class Animal {
    private int patas;

    public Animal(int patas) {
        this.patas = patas;
    }

    public void moverse() {
        System.out.println("El animal se est√° moviendo.");
    }

    public abstract void comer();
}

// Interfaz Mascota
interface Mascota {
    String obtenerNombre();

    void ponerNombre(String nombre);

    void jugar();
}